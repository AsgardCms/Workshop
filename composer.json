{
  "name": "asgardcms/workshop-module",
  "type": "asgard-module",
  "description": "Workshop module for AsgardCMS. Handles the activation of modules.",
  "keywords": [
    "asgardcms",
    "workshop",
    "modules"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Nicolas Widart",
      "email": "info@asgardcms.com",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "support@asgardcms.com",
    "issues": "https://github.com/AsgardCms/Workshop/issues",
    "source": "https://github.com/AsgardCms/Workshop"
  },
  "require": {
    "php": ">=5.4",
    "composer/installers": "~1.0",
    "asgardcms/core-module": "~1.0",
    "pingpong/modules": "dev-feature/5.1",
    "symfony/yaml": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "orchestra/testbench": "~3.1"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/nWidart/modules"
    }
  ],
  "autoload-dev": {
    "psr-4": {
      "Modules\\Workshop\\": ".",
      "Modules\\": "Modules/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
